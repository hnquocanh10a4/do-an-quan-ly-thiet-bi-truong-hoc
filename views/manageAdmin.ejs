<body>
    
    <main role="main">

        <!-- Main jumbotron for a primary marketing message or call to action -->
        <div class="manage"> 
        <div class="jumbotron">
          <div class="container">
            <h1 class="display-3">Quản Lý thiết bị</h1>
          </div>
        </div>
  
        <div class="container">
          <!-- Example row of columns -->
          <h1 class="titleDay">Hôm nay:</h1>
          <div class="row">
              
              <% loanManage.forEach(function(item){  %> 
              <div class="col-md-4">
              <h3><%=  item.MSSV  %></h3>
              <p>Họ và tên : <%=  item.studentName  %></p>
              <p>Tên phòng : <%=  item.roomName  %></p>
              <p>Số điện thoại:  <%=  item.phone  %></p>
              <p><a class="btn btn-info" href="/manageAdmin/<%=  item.MSSV  %>" role="button"><i class="far fa-sticky-note"></i> Chi tiết </a></p>
             <p><a class="btn btn-primary pricing" href="/pricing/<%=  item.MSSV  %>" role="button"><i class="fas fa-money-check-alt"></i> Mất mát </a></p>
            </div>
            <% }); %> 
          </div>
  
          <hr>
          <h1 class="titleDay">Chưa trả, hết hạn:</h1>
          <div class="row">
            
            <br>
            <% loanManage1.forEach(function(item){  %> 
            <div class="col-md-4">
            <h3 class="expiration"><%=  item.MSSV  %></h3>
            <p class="name-out-of-date">Họ và tên : <%=  item.studentName  %></p>
            <p>Tên phòng : <%=  item.roomName  %></p>
            <p>Số điện thoại:  <%=  item.phone  %></p>
            <p><a class="btn btn-info" href="/manageAdmin/<%=  item.MSSV  %>" role="button"><i class="far fa-sticky-note"></i> Chi tiết </a></p>
            <p><a class="btn btn-primary pricing" href="/pricing/<%=  item.MSSV  %>" role="button"><i class="fas fa-money-check-alt"></i> Mất mát </a></p>
          </div>
          <% }); %> 
        </div>
          
        </div>
  
        </div> <!-- /container -->
    </div>
  
